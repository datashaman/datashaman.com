{
    "metadata": {
        "site": {
            "title": "datashaman",
            "link": "https://datashaman.com"
        }
    },
    "plugins": {
        "markdown": true,
        "more": true,
        "permalinks": {
            "pattern": "posts/:date-:title/"
        },
        "collections": {
            "posts": "posts/*"
        },
        "feed": {
            "collection" : "posts",
            "feed": {
                "title": "datashaman",
                "link": "https://datashaman.com/"
            }
        },
        "sass": true,
        "views": {
            "paths": [
                "views"
            ],
            "compiled": ".cache"
        }
    }
}
